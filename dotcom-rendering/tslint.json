{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-react-a11y",
        "tslint-plugin-prettier",
        "tslint-config-prettier"
    ],
    "jsRules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "jsx-no-lambda": false,
        "interface-name": false,
        "prettier": true
    },
    "rulesDirectory": []
}