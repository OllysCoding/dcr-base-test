{
    "plugins": [
        "babel-plugin-transform-object-rest-spread",
        [
            "babel-plugin-transform-runtime",
            {
                "polyfill": false
            }
        ]
    ],
    "env": {
        "es": {
            "presets": [
                "babel-preset-react",
                [
                    "babel-preset-env",
                    {
                        "modules": false
                    }
                ]
            ]
        },
        "cjs": {
            "presets": [
                "babel-preset-react",
                [
                    "babel-preset-env",
                    {
                        "targets": {
                            "node": "current"
                        }
                    }
                ]
            ]
        }
    }
}
